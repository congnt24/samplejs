{
  "containerDefinitions": [
    {
      "image": "%REPOSITORY_URI%",
      "name": "%SERVICE_NAME%",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "memory": "128",
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "128"
}
